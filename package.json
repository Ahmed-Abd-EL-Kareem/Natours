    "bcryptjs": "^3.0.2",
    "compression": "1.7",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.21.2",
    "express-mongo-sanitize": "^1.3.2",
    "express-rate-limit": "^3.5.3",
    "helmet": "^3.23.3",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^6.17.0",
    "mongoose": "^5.13.23",
