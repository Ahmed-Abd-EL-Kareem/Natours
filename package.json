    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.37.5",
    "parcel-bundler": "^1.12.5",
    "prettier": "^1.19.1"
